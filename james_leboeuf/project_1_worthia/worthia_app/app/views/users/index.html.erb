<div class="row">
  <% @users.each do |user| %>
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <img data-src="holder.js/300x300" src="<%= user.image %>">
        <div class="caption">
          <h3><%= user.name %></h3>
          <p><%=  %></p>
          <p><a href="users/<%= user.id %>" class="btn btn-primary" role="button"><%= user.name %></a></p>
        </div>
      </div>
    </div>
  <% end %>
</div>
 
