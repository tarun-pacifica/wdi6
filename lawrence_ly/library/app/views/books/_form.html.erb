<%= form_for @book do |f| %>
<p>
  <%= f.label :title %>
  <%= f.text_field :title %>
</p>
<p>
  <%= f.label :year %>
  <%= f.text_field :year %>
</p>
<p>
  <%= f.label :pages %>
  <%= f.text_field :pages %>
</p>
<p>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
</p>
<p>
  <%= f.label :image %>
  <%= f.text_field :image %>
</p>
<label></label>
<%= f.submit %>
<% end %>


