<%= form_for @author do |f| %>
<p>
  <%= f.label :name %>
  <%= f.text_field :name %>
</p>
<p>
  <%= f.label :gender %>
  <%= f.text_field :gender %>
</p>
<p>
  <%= f.label :dob %>
  <%= f.text_field :dob %>
</p>
<p>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
</p>
<p>
  <%= f.label :image%>
  <%= f.text_field :image%>
</p>
<label></label>
<%= f.submit %>
<% end %>

