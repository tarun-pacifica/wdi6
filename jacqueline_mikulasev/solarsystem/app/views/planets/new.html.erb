<form action="/planets" method="post">
	<label>Name:</label>
	<input type="text" name="planet[name]" autofocus>
	<label>Image:</label>
	<input type="text" name="planet[image]">
	<label>Orbit:</label>
	<input type="text" name="planet[orbit]">
	<label>Diameter:</label>
	<input type="text" name="planet[diameter]">
	<label>Mass:</label>
	<input type="text" name="planet[mass]">
	<label>Moons:</label>
	<input type="text" name="planet[moons]">
	<button>Planet Me!</button>
</form>